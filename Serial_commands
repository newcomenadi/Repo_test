<<======================><======================>>Miscellaneous<<======================><======================>>
setenv -p bd "memload boot cm; sleep 1500; memload kernel; bootram -samelf -loader=smtpx ram:0xc1000000 'root=/dev/sda4 rw"
setenv -p STARTUP "$bn"
setenv -p STARTUP 'boot...'  or  setenv -p STARTUP "boot..."
printenv
unsetenv STARTUP


